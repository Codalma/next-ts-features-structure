{
  "name": "next-ts-clean-ddd",
  "description": "This is a Next TS template for building a clean DDD UI",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "author": "Adelino ARAUJO",
  "private": true,
  "version": "0.1.0",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Codalma/next-ts-clean-ddd.git"
  },
  "keywords": [
    "Next.js",
    "TypeScript",
    "clean architecture",
    "DDD",
    "UI",
    "testing",
    "linting",
    "preprocessing",
    "icons",
    "environment variables",
    "Git hooks",
    "dependency management",
    "continuous integration",
    "containerization"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "husky": "husky install",
    "lint": "eslint ./ --fix",
    "format": "prettier --write .",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-all": "npm run format && npm run check-lint && npm run check-types",
    "clean": "rm -rf coverage/ .next/ node_modules/ out .next/",
    "docker:dev": "docker compose -f docker-compose.yml -f docker-compose-dev.yml up",
    "docker:test": "docker compose -f docker-compose.yml -f docker-compose-test.yml up",
    "docker:prod": "docker compose -f docker-compose.yml -f docker-compose-prod.yml up"
  },
  "dependencies": {
    "next": "13.4.3",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.2.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "cypress": "^12.12.0",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
